<script type="text/javascript">
    $(document).ready(function(){
        window_resize();
        $(window).resize(function(){
            window_resize();
        });
    });
    function window_resize(){
        var height_window = $(window).innerHeight();
        $("#banner").css("height",height_window+"px");
    }
</script>
<script type="text/javascript">
    $(document).ready(function(){
        function loginPopup(){
        	var formLogin = $("#accountLogin").html();
        	var loginTitle = "Đăng nhập";
        	<?php if(Yii::app()->controller->action->id=='register'):?>
        		loginTitle = "Đăng ký học";
        	<?php endif;?>
            popup({
                width:"420px",
                title: loginTitle,
                content:function(event){
                    return formLogin;
                }
            });
        }
        <?php if(isset($openSigninPopup) && $openSigninPopup):?>
        loginPopup();//Auto open login popup
        <?php endif;?>
        $(document).on("click",".accountLogin",function(){
            <?php if(isset(Yii::app()->user->id)):?>
            	window.location = '<?php echo Yii::app()->baseUrl; ?>/site/loggedRedirect';
            <?php else:?>
        		loginPopup();
        	<?php endif;?>
        });
	
        $(document).on("click","button",function(){
            $(".myForm").ajaxForm(function(response){
                if(response.success){
                    $(response.htmlTag).html('<div class="alert alert-success">'+response.notice+'</div>');
                }else{
                    $(response.htmlTag).html('<div class="alert alert-danger">'+response.notice+'</div>');
                }
                loading.removed();
            });
        });
		$(document).on("click",".oauthPopup",function(){
			removePopupByID('popupAll');
		});
		$(document).on("click",".icon-remove-sign",function(){
			removePopupByID('popupAll');
			window.location = '<?php echo Yii::app()->baseurl.'/news'?>';
		});
		$(document).on("click","#popupAll"+" #popupContent",function(event){
            var tag = event.target;
            var id =$(tag).attr("id");
            if(id=="popupContent"){
                removePopupByID('popupAll');
				window.location = '<?php echo Yii::app()->baseurl.'/news'?>';
            }
        });
    });
</script>
